{
  "name": "@quasar/quasar-ui-qoverlay",
  "version": "1.0.0",
  "author": "Jeff Galbraith <jeff@quasar.dev>",
  "description": "QOverlay is an app extension for Quasar for making overlays",
  "license": "MIT",
  "module": "dist/index.esm.js",
  "main": "dist/index.common.js",
  "scripts": {
    "lint": "eslint --ext .js,.vue src",
    "lint-fix": "eslint --ext .js,.vue src --fix",
    "dev": "cd dev; yarn dev; cd ..",
    "dev:umd": "yarn build && node build/script.open-umd.js",
    "dev:ssr": "cd dev; yarn 'dev:ssr'; cd ..",
    "dev:ios": "cd dev; yarn 'dev:ios'; cd ..",
    "dev:android": "cd dev; yarn 'dev:android'; cd ..",
    "dev:electron": "cd dev; yarn 'dev:electron'; cd ..",
    "build": "node build/index.js",
    "build:js": "node build/script.javascript.js",
    "build:css": "node build/script.css.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quasarframework/quasar-ui-qoverlay.git"
  },
  "bugs": "https://github.com/quasarframework/quasar-ui-qoverlay/issues",
  "homepage": "https://github.com/quasarframework/quasar-ui-qoverlay",
  "keywords": [
    "quasar",
    "app",
    "app extension",
    "extension",
    "overlay"
  ],
  "devDependencies": {
    "@rollup/plugin-buble": "^0.20.0",
    "@rollup/plugin-json": "^4.0.0",
    "@vue/eslint-config-standard": "^5.0.1",
    "autoprefixer": "^9.7.3",
    "chalk": "^3.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.7.2",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-vue": "^6.0.1",
    "fs-extra": "^8.1.0",
    "node-sass": "^4.13.0",
    "open": "^7.0.0",
    "postcss": "^7.0.24",
    "postcss-rtl": "^1.5.0",
    "quasar": "^1.5.5",
    "quasar-json-api": "^0.0.13",
    "rimraf": "^3.0.0",
    "rollup": "^1.27.8",
    "rollup-plugin-node-resolve": "^5.2.0",
    "uglify-es": "^3.3.9",
    "zlib": "^1.0.5"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ]
}
